    "@types/react-dom": "^17.0.5",
    "autoprefixer": "10.x",
    "axios": "^0.20.0",
    "cross-env": "7.x",
    "npm-run-all": "4.x",
    "postcss": "8.x",
    "postcss-cli": "8.x",
